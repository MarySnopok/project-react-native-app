{
  "expo": {
    "name": "magic-ball",
    "slug": "magic-ball-native-app",
    "sdkVersion": "43.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/askme-icon.png",
    "splash": {
      "image": "./assets/askme.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marysnopok.magicball.reactnative"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/askme.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.marysnopok.reactnative.magicball"
    },
    "web": {
      "favicon": "./assets/askme.png"
    }
  }
}
