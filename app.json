{
	"expo": {
		"name": "magic-ball",
		"slug": "magic-ball-native-app",
		"sdkVersion":"43.0.0",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/askme.jpeg",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage":"./assets/askme.jpeg",
				"backgroundColor": "#FFFFFF"
			}
		},
		"web": {
			"favicon": "./assets/askme.jpeg"
		}
	}
}
